{
    "name": "michaeltintiuc/laravel-permy",
    "description": "Laravel user permissions (roles or groups) based on defined routes.",
    "license": "MIT",
    "keywords": ["laravel", "permission", "role", "user", "route", "access", "control", "acl", "group"],
    "authors": [
        {
            "name": "Michael Tintiuc",
            "email": "contact@michaeltintiuc.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.2.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "MichaelT\\Permy\\": "src"
        },
        "files": [
            "src/exceptions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MichaelT\\Permy\\PermyServiceProvider"
            ],
            "aliases": {
                "Permy": "MichaelT\\Permy\\PermyFacade"
            }
        }
    }
}
